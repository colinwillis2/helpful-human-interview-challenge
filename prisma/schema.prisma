generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model hex_colors {
  id       String  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name     String?
  hex_code String?
  slug     String? @unique
}
